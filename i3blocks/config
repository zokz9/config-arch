# Основные настройки
command=$SCRIPT_DIR/$BLOCK_NAME/$BLOCK_NAME
separator_block_width=20
separator_block_heigh=50
markup=none

# Модули
[time]
command=date '+%H:%M:%S'
interval=1

[memory]
command=~/.config/i3blocks/memory
interval=1

[network]
command=~/.config/i3blocks/network
interval=1

[battery]
command=~/.config/i3blocks/battery
interval=1

[volume]
command=~/.config/i3blocks/volume
interval=1

[label]
command=echo 'Arch Linux '
interval=once
align=center
color=#ffffff
